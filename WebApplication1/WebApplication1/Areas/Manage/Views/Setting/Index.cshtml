@model List<Setting>
@{
    int count = 0;

}
<div class="row">
    <div class="col-lg-6 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title">Feauture Table</h4>
                <a class="btn btn-primary" asp-controller="Setting" asp-action="Create">Create</a>
                <p class="card-description">
                    Add class <code>.table</code>
                </p>
                <div class="table-responsive">
                    <table class="table">
                        <thead>
                            <tr>
                                <th>No</th>
                                <th>Key</th>
                                <th>Value</th>
                                <th>Settings</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                count++;
                                <tr>
                                    <td>@count</td>
                                    <td>@item.Key</td>
                                    <td>@item.Value</td>

                                    <td>
                                        <a class="btn btn-primary" asp-controller="setting" asp-action="Update" asp-route-id="@item.Id">Update</a>
                                        <a class="btn btn-primary" asp-controller="setting" asp-action="Delete" asp-route-id="@item.Id"> Delete</a>
                                    </td>
                                </tr>
                            }


                        </tbody>
                    </table>

                </div>
            </div>
        </div>
    </div>
</div>
